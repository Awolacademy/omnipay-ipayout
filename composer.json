{
    "name": "awolacademy/omnipay-ipayout",
    "description": "iPayout driver for the Omnipay PHP payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase",
        "ipayout"
    ],
    "homepage": "https://github.com/awolacademy/omnipay-ipayout",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Lis",
            "email": "knightar@gmail.com",
            "homepage": "http://artician.net",
            "role": "Developer"
        },
        {
            "name": "Jeffrey Brown",
            "email": "jb@bogart.productions",
            "homepage": "http://bogart.productions",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "omnipay/common": "~2.0"
    },
    "require-dev": {
        "omnipay/tests": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\iPayout\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\iPayout\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "../../bin/phpunit"
    },
    "repositories": [
        {
            "type": "vcs",
            "no-api": true,
            "url": "git@github.com:KnightAR/omnipay-tests.git"
        }
    ]
}

